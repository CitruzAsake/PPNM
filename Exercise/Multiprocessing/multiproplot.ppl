set terminal pdf dpi 800
set output "multiproplot.pdf"
set key outside
set grid
set xlabel "$x$"
set ylabel "$y$"
set xrange [0:1]
set yrange [0:1]
set title "Monte Carlo Method plot "
plot \
"circledatafile.txt" using 1:2 w l lt 10 lw 4 color blue  title "circular data values $r=1$" 			\
,"datafile.txt" using 1:2 w p pt 31 lw 4 color cyan  title "Values generated by random" 			\
